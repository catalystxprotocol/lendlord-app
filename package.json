    "bignumber.js": "^9.1.1",
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.32",
    "json-bigint": "^1.0.0",
    "move-decimal-point": "^0.0.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.30",
    "pinia-plugin-persistedstate": "^3.0.2",
    "swiper": "^9.0.5",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5",
    "web3": "^1.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
